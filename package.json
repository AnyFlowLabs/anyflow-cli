    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "open": "^10.1.0",
    "pkg-dir": "^8.0.0",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.5.3",
    "@types/openurl": "^1.0.3",
    "@types/pkg-dir": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
